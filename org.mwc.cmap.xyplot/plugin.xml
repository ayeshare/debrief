<plugin>

     <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="org.mwc.cmap.plotViewer.CMAP"
            class="org.mwc.cmap.xyplot.views.XYPlotView"
            icon="icons/document_chart.png"
            id="org.mwc.cmap.xyplot.views.XYPlotView"
            name="XY Plot" restorable="false">
      </view>
   </extension>
   
   <extension
         point="org.mwc.cmap.core.RightClickSupport">
      <helper
            class="org.mwc.cmap.xyplot.XYPlotGeneratorButtons">
      </helper>
   </extension>
   
   
   <extension
       point="org.eclipse.ui.views">
        <view
            allowMultiple="true"
            category="org.mwc.cmap.plotViewer.CMAP"
            class="org.mwc.cmap.xyplot.views.CrossSectionView"
            icon="icons/x-section.png"
            id="org.mwc.cmap.xyplot.views.CrossSectionView"
            name="Cross Section">
      </view>
   </extension>
</plugin>
